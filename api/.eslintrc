{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-underscore-dangle": [
      1,
      {
        "allow": [
          "_id",
          "__v"
        ]
      }
    ],
    "class-methods-use-this": "off",
    "no-plusplus": "off",
    "no-await-in-loop": "off",
    "func-names": "off",
    "no-param-reassign": "off",
    "no-restricted-syntax": "off",
    "no-console": [
      "error",
      {
        "allow": [
          "log",
          "info",
          "warn",
          "error"
        ]
      }
    ]
  }
}